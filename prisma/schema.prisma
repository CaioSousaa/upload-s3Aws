generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Upload {
  id         String @id @default(uuid()) @map("_id") @db.ObjectId
  size       Int
  key        Int
  url        String
  created_at DateTime @default(now())

  @@map("upload")
}